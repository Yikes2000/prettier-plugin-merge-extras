{
  "private": true,
  "name": "root",
  "version": "0.0.0",
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "pnpm -r exec rm -rf .turbo && pnpm -r exec rm -rf dist && pnpm -r exec rm -rf node_modules",
    "build": "turbo run build --no-daemon --filter bundle-entry",
    "format": "turbo run format --no-daemon",
    "format:fix": "turbo run format:fix --no-daemon",
    "test:v3": "turbo run test --no-daemon --filter ./tests/v3-* --filter !./tests/v3-loop"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "eslint": "8.35.0",
    "prettier": "3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "1.10.14",
    "typescript": "4.9.5"
  },
  "packageManager": "pnpm@7.33.0"
}
