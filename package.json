{
  "private": true,
  "name": "root",
  "version": "0.0.0",
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "pnpm -r exec rm -rf .turbo && pnpm -r exec rm -rf dist && pnpm -r exec rm -rf node_modules",
    "format": "turbo run format --no-daemon --filter=!./tests/*-v3-plugin",
    "format:fix": "turbo run format:fix --no-daemon --filter=!./tests/*-v3-plugin",
    "lint": "eslint tests",
    "test:perf": "turbo run test --force --no-daemon --filter=performance-test",
    "test:v2": "turbo run test --no-daemon --filter=./tests/v2-* --filter=!./tests/*-v3-plugin"
  },
  "dependencies": {
    "prettier-plugin-merge": "0.3.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "prettier": "~2.8.4",
    "turbo": "1.10.14",
    "typescript": "4.9.5"
  },
  "packageManager": "pnpm@7.33.0"
}
